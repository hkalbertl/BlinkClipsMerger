  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>disable</Nullable>
    <Title>BlinkClipsMerger</Title>
    <Authors>Albert L</Authors>
    <PackageProjectUrl>https://github.com/hkalbertl/BlinkClipsMerger</PackageProjectUrl>
